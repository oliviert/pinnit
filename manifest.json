{
  "name": "Pinnit",
  "version": "1.0",
  "description": "Customize your subreddit bar on Reddit",
  "content_scripts": [
    {
      "matches": [
        "http://reddit.com/*",
        "https://reddit.com/*",
        "http://*.reddit.com/*",
        "https://*.reddit.com/*"
      ],
      "js": ["jquery-1.7.2.min.js", "pin.js"]
    }
  ]
}

